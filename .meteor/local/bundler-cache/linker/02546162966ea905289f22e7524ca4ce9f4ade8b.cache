 } else {                                                                                                 //\n          if (input != input2) {                                                                                 // 18\n            alert(\"Password Mismatch\");                                                                          // 19\n          } else {                                                                                               //\n            Accounts.createUser({                                                                                // 23\n              username: t.find('#signup-username').value,                                                        // 24\n              password: t.find('#signup-password').value,                                                        // 25\n              email: t.find('#signup-email').value,                                                              // 26\n              profile: {                                                                                         // 27\n                confirm: t.find('#signup-confirm').value,                                                        // 28\n                current_response_id: \"\"                                                                          // 29\n              }                                                                                                  //\n                                                                                                                 //\n            });                                                                                                  //\n                                                                                                                 //\n            t.find('#signup-username').value = \"\";                                                               // 34\n            t.find('#signup-password').value = \"\";                                               