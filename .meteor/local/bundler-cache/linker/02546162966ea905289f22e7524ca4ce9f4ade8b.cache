sion.set('selectedQuestion', '1');                                                                    // 87\n        changeAnswer('1');                                                                                       // 88\n      }                                                                                                          //\n    });                                                                                                          //\n                                                                                                                 //\n    Meteor.call(\"getTimer\", function (error, result) {                                                           // 94\n      if (error) console.log(error.reason);else {                                                                // 95\n        self.time_left.set(result);                                                                              // 98\n      }                                                                                                          //\n    });                                                                                                          //\n                                                                                                                 //\n    Meteor.setInterval(function () {                                                                             // 102\n      self.time_left.set(self.time_left.get() - 1);                                                              // 102\n    }, 1000);                                                                                                    //\n    Meteor.setInterval(function () {                                                                             // 103\n      Meteor.call('setTimer', self.time_left.get());                                                             // 103\n    }, 5000);                                                                                                    //\n  });                                                                                                            //\n                                                                                                                 //\n  Template.questionSelect.events({                                                                               // 106\n    'click input': function () {                                                                                 // 107\n      function clickInput(event, template) {                                                                     // 107\n        Session.set('selectedQuestion', event.target.value);                                                     // 108\n        changeAnswer(event.target.value);                                                                        // 109\n      }                                                                                                          //\n                                                                                                                 //\n      return clickInput;                                                                                         //\n    }()                                                                                                          //\n  });                                                                                                            //\n                                                                                                                 //\n  Template.online.helpers({                                                                                      // 113\n    'showGroupQuizzes': function () {                                                                            // 114\n      function showGroupQuizzes() {                                                                              // 114\n        var groupQuizzes = ThisResponse.find({ type: 'Group' }).fetch();                                         // 115\n        var time = new Date();                                                                                   // 116\n        for (i = 0; i < groupQuizzes.length; i++) {                                                              // 117\n          var date = new Date(Date.parse(time) - Date.parse(groupQuizzes[i].createdAt));                         // 118\n          if (date.getMinutes() > 30) {                                                                          // 119\n            groupQuizzes.splice(i, 1);                                                                           // 120\n          }                                                                                                      //\n        }                                                                                                        //\n        return groupQuizzes;                                                                                     // 123\n      }                                                                                                          //\n                                                                                                                 //\n      return showGroupQuizzes;                                                                                   //\n    }()                                                                                                          //\n  });                                                                                                            //\n                                                                                                                 //\n  Template.questionSelect.helpers({                                                                              // 127\n    time_left: function () {                                                                                     // 128\n      function time_left() {                                