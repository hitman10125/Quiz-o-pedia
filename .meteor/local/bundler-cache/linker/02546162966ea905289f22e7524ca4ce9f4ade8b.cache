zQuestions[parseInt(Session.get('selectedQuestion')) - 1];                                     // 151\n      }                                                                                                          //\n                                                                                                                 //\n      return questionData;                                                                                       //\n    }()                                                                                                          //\n                                                                                                                 //\n  });                                                                                                            //\n                                                                                                                 //\n  /* **************************************************************************************************** */     //\n                                                                                                                 //\n  Template.quizSelect.events({                                                                                   // 4\n    \"submit .quizParameters\": function () {                                                                      // 159\n      function submitQuizParameters(event) {                                                                     // 159\n        event.preventDefault();                                                                                  // 160\n        var selectedType = event.target.quizType.value;                                                          // 161\n        var selectedCategory = event.target.quizCategory.value;                                                  // 162\n        Meteor.call(\"createResponse\", selectedType, selectedCategory);                                           // 163\n      }                                                                                                          //\n                                                                                                                 //\n      return submitQuizParameters;                                                                               //\n    }(),                                                                                                         //\n    'click #logoutbtn': function () {                                                                            // 165\n      function clickLogoutbtn() {                                                                                // 165\n                                                                                                                 //\n        Meteor.logout();                                                                                         // 167\n      }                                                                                                          //\n                                                                                                                 //\n      return clickLogoutbtn;                                                                                     //\n    }()                                                                                                          //\n                                                                                                                 //\n  });                                                                                                            //\n                                                                                                                 //\n  /* **************************************************************************************************** */     //\n}                                                                                                                // 4\n                                                                                                                 //\nRouter.configure({                                                                                               // 175\n  layoutTemplate: 'ApplicationLayout'                                                                            // 176\n});                                                                                                              //\n                                                                                                                 //\nRouter.route('/', function () {                                                                                  // 179\n  if (!Meteor.userId()) {                                                                                        // 180\n    this.render('appBody', { to: \"template1\" });                                                                 // 181\n    this.render('emptyTemplate', { to: \"template2\" });                                                           // 182\n  } else if (Meteor.user().profile.current_response_id == '') {                                                  //\n    this.render('emptyTemplate', { to: \"template1\" });                                                           // 185\n    this.render('quizSelect', { to: \"template2\" });                                                              // 186\n  } else {                                                                                                       //\n    this.render('questionSelect', { to: \"template1\" });                                                          // 189\n    this.render('questionShow', { to: \"template2\" });                                                            // 190\n  }                                                                                                              //\n});                                                                                                              //\n                                                                                                                 //\n/* **************************************************************************************************** */       //\n                                                                                                                 //\nif (Meteor.isServer) {                                                                                           // 196\n                                                                                                                 //\n  Meteor.methods({                                                                                               // 198\n                                                                                                                 //\n    getQuiz: function () {                                                                                       // 200\n      function getQuiz() {                                                                                       // 200\n        var responseID = Meteor.user().profile.current_response_id;                                              // 201\n        var quizID = ThisResponse.findOne(responseID).quizID;                                                    // 202\n        var randomQuiz = ThisQuiz.findOne(quizID);                                                               // 203\n        var quizQuestions = randomQuiz.quiz;                                                                     // 204\n        for (var i = 0; i < quizQuestions.length; i++) {                                                         // 205\n          delete quizQuestions[i][\"answer\"];                                                                     // 206\n        }                                                                                                        //\n        return quizQuestions;                                                                                    // 208\n      }                                                                                                          //\n                                                                                                                 //\n      return getQuiz;                                                                                            //\n    }(),                                                                                                         //\n                                                                                                                 //\n    getRandomQuizID: function () {                                                                               // 211\n      function getRandomQuizID(category) {                                                                       // 211\n        var randomQuiz = ThisQuiz.find({ category: category }).fetch();                                          // 212\n        randomQuiz = randomQuiz[Math.floor(Math.random() * randomQuiz.length)];                                  // 213\n        return randomQuiz._id;                                                                                   // 214\n      }                                                                                                          //\n                                                                                                                 //\n      return getRandomQuizID;                                                                                    //\n    }(),                                                                                                         //\n                                                                                                                 //\n    createResponse: function () {                                                                                // 217\n      function createResponse(type, category) {                                                                  // 217\n        var _answers;                                                                                            //\n                                                                                                                 //\n        if (!Meteor.userId()) {                                                                                  // 218\n          throw new Meteor.Error('not-authorized');                                                              // 218\n        }                                                                                                        //\n                                                                                                                 //\n        var quizID = Meteor.call('getRandomQuizID', category);                                                   // 220\n                                                                                                                 //\n        var responseID = ThisResponse.insert({                                                                   // 222\n          userID: [Meteor.userId()],                                                                             // 223\n          category: category,                                                                                    // 224\n          quizID: quizID,                                                                                        // 225\n          type: type,                                                                                            // 226\n          answers: (_answers = {}, _answers[Meteor.userId()] = {}, _answers),                                    // 227\n          time_left: 1200,                                                                                       // 228\n          score: {},                                                                                    