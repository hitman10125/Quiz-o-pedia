                    // 353\n                                                                                                                       //\n        var arr1 = [];                                                                                                 // 355\n        var arr2 = [];                                                                                                 // 356\n        var arr3 = [];                                                                                                 // 357\n        var arr4 = [];                                                                                                 // 358\n        var arr5 = [];                                                                                                 // 359\n        var arr6 = [];                                                                                                 // 360\n        var arr7 = [];                                                                                                 // 361\n        var arr8 = [];                                                                                                 // 362\n        var arrayFinal = [];                                                                                           // 363\n        for (var i = 0; i < 2; i++) {                                                                                  // 364\n          arr1[i] = Session.get('selectedScore')[i].question;                                                          // 366\n          //  console.log('arr1 '+arr1[i]);                                                                            //\n          arr2[i] = Session.get('selectedScore')[i].option1;                                                           // 365\n          arr3[i] = Session.get('selectedScore')[i].option2;                                                           // 369\n          arr4[i] = Session.get('selectedScore')[i].option3;               