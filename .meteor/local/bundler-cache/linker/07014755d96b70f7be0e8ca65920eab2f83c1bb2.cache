ar('subscribed');                                                                                 // 238\n        }                                                                                                              //\n        return minutes + \":\" + seconds;                                                                                // 240\n      }                                                                                                                //\n                                                                                                                       //\n      return time_left;                                                                                                //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  /* **************************************************************************************************** */           //\n                                                                                                                       //\n  Template.questionShow.events({                                                                                       // 4\n    'click input': function () {                                                                                       // 247\n      function clickInput(event, template) {                                                                           // 247\n        Meteor.call('setResponse', parseInt(Session.get('selectedQuestion')), event.target.value);                     // 248\n      }                                                                                                                //\n                             