 Session.set('selectedScore2', result);                                                                     // 297\n            console.log(Session.get('selectedScore2'));                                                                // 298\n            console.log(\"aaasssssss\" + Session.get('selectedScore2'));                                                 // 299\n          }                                                                                                            //\n        });                                                                                                            //\n        Router.go('scoreCard');                                                                                        // 302\n      }                                                                                                                //\n                                                                                                                       //\n      return clickOnsubmitbutton;                                                                                      //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionShow.helpers({                                                                                      // 306\n    questionData: function () {                                                                                        // 307\n      function questionData() {                                                                                        // 307\n        return quizQuestions[parseInt(Session.get('selectedQuestion')) - 1];                                           // 308\n      }  