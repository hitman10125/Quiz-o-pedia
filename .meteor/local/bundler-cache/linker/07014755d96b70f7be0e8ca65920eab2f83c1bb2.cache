lue);                                                           // 166\n        changeAnswer(event.target.value);                                                                              // 167\n      }                                                                                                                //\n                                                                                                                       //\n      return clickInput;                                                                                               //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.helpers({                                                                                            // 171\n    'showGroupQuizzes': function () {                                                                                  // 172\n      function showGroupQuizzes() {                                                                                    // 172\n        var groupQuizzes = ThisResponse.find({ type: 'Group' }).fetch();                                               // 173\n        var time = new Date();                                                                                         // 174\n        for (i = 0; i < groupQuizzes.length; i++) {                                                                    // 175\n          var date = new Date(Date.parse(time) - Date.parse(groupQuizzes[i].createdAt));                               // 176\n          if (date.getMinutes() > 30) {                                                                                // 177\n            groupQuizzes.splice(i, 1);                                                                                 // 178\n          }                                                                                                            //\n        }                                                                                                              //\n        return groupQuizzes;                                                                                           // 181\n      }                                                                                                                //\n                                                                                                                       //\n      return showGroupQuizzes;                                                                                         //\n    }(),                                                                                                               //\n    'totalGroupQuizzes': function () {                                                                                 // 183\n      function totalGroupQuizzes() {                                                                                   // 183\n        var groupQuizzes = ThisResponse.find({ type: 'Group' }).fetch();                                               // 184\n        var time = new Date();                                                                                         // 185\n        for (i = 0; i < groupQuizzes.length; i++) {                                                                    // 186\n          var date = new Date(Date.parse(time) - Date.parse(groupQuizzes[i].createdAt));                               // 187\n          if (date.getMinutes() > 30) {                                                                                // 188\n            groupQuizzes.splice(i, 1);                                                                                 // 189\n          }                                                                                                            //\n        }                                                                                                              //\n        return groupQuizzes.length > 0;                                                                                // 192\n      }                                                                                                                //\n                                                                                                                       //\n      return totalGroupQuizzes;                                                                                        //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.events({                                                                                             // 196\n    'click input': function () {                                                                                       // 197\n      function clickInput() {                                                                                          // 197\n        event.preventDefault();                                                                                        // 198\n        // var quizID = $(event.target).attr('id');                                                                    //\n        // var userID = $(event.target).attr('name');                                                                  //\n        var responseID = $(event.target).attr('id');                                                                   // 197\n        var subscribed = $(event.target).attr('value');                                                                // 202\n        console.log(subscribed);                                                                                       // 203\n        Session.setPersistent('subscribed', subscribed);                                                               // 204\n        // var category = ThisResponse.find({userID: userID, quizID: quizID}).fetch()[0].category;                     //\n        // var timeLeft = ThisResponse.find({userID: userID, quizID: quizID}).fetch()[0].waiting_time;                 //\n        // console.log(quizID+\"  \"+userID+\"  \"+category+\"  \"+timeLeft+\"  \"+responseID);                                //\n        Meteor.call('setGroupResponseId', responseID);                                                                 // 197\n      }                                                                                                                //\n                                                                                                                       //\n      return clickInput;                                                                                               //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionSelect.helpers({                                                                                    // 212\n    time_left: function () {                                                                                           // 213\n      function time_left() {                                                                                           // 213\n        var time = Template.instance().time_left.get();                                                                // 214\n        Session.set('timeLeft', time);                                                                                 // 215\n        var minutes = (Math.floor(time / 60) < 10 ? '0' : '') + Math.floor(time / 60);                                 // 216\n        var seconds = (time % 60 < 10 ? '0' : '') + time % 60;                                                         // 217\n        if (time <= 0) {                                                                                               // 218\n          setTimeout(clearInterval(updateTimeLeft), 1000);                                                             // 219\n          setTimeout(clearInterval(updateWaitingTimeLeft), 1000);                                                      // 220\n        }                                                                                                              //\n        return minutes + \":\" + seconds;                                                                                // 222\n      }                                                                                                                //\n                                                                                                                       //\n      return time_left;                                                                                                //\n    }(),                                                                                                               //\n    'ended': function () {                                                                                             // 224\n      function ended() {                                                                                               // 224\n        return Session.get('timeLeft') <= 0;                                                                           // 225\n      }                      