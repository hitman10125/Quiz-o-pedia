eft;                                                             // 289\n      }                                                                                                                //\n                                                                                                                       //\n      return getTimer;                                                                                                 //\n    }(),                                                                                                               //\n    endquiz: function () {                                                                                             // 291\n      function endquiz(id) {                                                                                           // 291\n        Meteor.users.update({ _id: id }, { $set: { \"profile.current_response_id\": \"\" } });                             // 292\n      }                                                                                                                //\n                                                                                                                       //\n      return endquiz;                                                                                                  //\n    }(),                                                                                                               //\n                                                                                                                       //\n    setWaitingTimer: function () {                                                                                     // 295\n      function setWaitingTimer(new_time) {                                                                             // 295\n        var responseID = Meteor.user().profile.current_response_id;                                                    // 296\n        var response = ThisResponse.findOne(responseID