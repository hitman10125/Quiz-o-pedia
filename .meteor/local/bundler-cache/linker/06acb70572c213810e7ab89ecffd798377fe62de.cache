lt) {                                                       // 293\n          if (error) {                                                                                                 // 294\n                                                                                                                       //\n            console.log(error.reason);                                                                                 // 296\n          } else {                                                                                                     //\n                                                                                                                       //\n            Session.set('selectedScore2', result);                                                                     // 301\n            console.log(Session.get('selectedScore2'));                                                                // 302\n            console.log(\"aaasssssss\" + Session.get('selectedScore2'));                                                 // 303\n          }                                                                                                            //\n        });                                                                                                            //\n        //Router.go('scoreCard');                                                                                      //\n        Session.set('isTimeUp', true);                                                                                 // 265\n      }                                                                                                                //\n                                                                                                                       //\n      return clickOnsubmitbutton;                                                                                      //\n    }()                                                      