                                            // 188\n        var time = new Date();                                                                                         // 189\n        for (i = 0; i < groupQuizzes.length; i++) {                                                                    // 190\n          var date = new Date(Date.parse(time) - Date.parse(groupQuizzes[i].createdAt));                               // 191\n          if (date.getMinutes() > 30) {                                                                                // 192\n            groupQuizzes.splice(i, 1);                                                                                 // 193\n          }                                                                                                            //\n        }                                                                                                              //\n        return groupQuizzes.length > 0;                                                                                // 196\n      }                                                                                                                //\n                                                                                                                       //\n      return totalGroupQuizzes;                                                                                        //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.events({                                                                                             // 200\n    'click input': function () {                                           