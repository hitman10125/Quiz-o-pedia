                                        //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionSelect.events({                                                                                     // 168\n    'click input': function () {                                                                                       // 169\n      function clickInput(event, template) {                                                                           // 169\n        Session.set('selectedQuestion', event.target.value);                                                           // 170\n        changeAnswer(event.target.value);                                                                              // 171\n      }                                                                                                                //\n                                                                                                                       //\n      return clickInput;                                                                                               //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.helpers({                                                                                            // 175\n    'showGroupQuizzes': function () {                                                                                  // 176\n      function showGroupQuizzes() {                                                