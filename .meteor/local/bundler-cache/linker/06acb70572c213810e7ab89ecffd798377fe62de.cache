                                                            //\n    }(),                                                                                                               //\n    'click #logoutbtn': function () {                                                                                  // 334\n      function clickLogoutbtn() {                                                                                      // 334\n        Meteor.call('endquiz', Meteor.userId());                                                                       // 335\n        // clearInterval('updateTimeLeft');                                                                            //\n        Meteor.logout();                                                                                               // 334\n      }                                                                                                                //\n                                                                                                                       //\n      return clickLogoutbtn;                                                                                           //\n    }()                                                                                                                //\n                                                                                                                       //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.scoreCard.events({                                                                                          // 342\n                                                                                                                       //\n    'click #logoutbtn': function () {                                  