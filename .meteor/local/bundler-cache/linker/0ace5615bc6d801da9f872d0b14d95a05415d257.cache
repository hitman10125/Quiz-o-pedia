                                           // 251\n        var time = Template.instance().time_left.get();                                                                // 252\n        Session.set('timeLeft', time);                                                                                 // 253\n        var minutes = (Math.floor(time / 60) < 10 ? '0' : '') + Math.floor(time / 60);                                 // 254\n        var seconds = (time % 60 < 10 ? '0' : '') + time % 60;                                                         // 255\n        if (time <= 0) {                                                                                               // 256\n          Meteor.clearInterval(Session.get('updateWaitingTimeLeft'));                                                  // 257\n          Meteor.clearInterval(Session.get('updateTimeLeft'));                                                         // 258\n          Session.clear('final_answer');                                                                               // 259\n          Session.clear('subscribed');                                                                                 // 260\n        }                                                                                                              //\n        return minutes + \":\" + seconds;                                                                                // 262\n      }                                                                                                                //\n                                                                                                                       //\n      return time_left;                                                                                                //\n    }()                                                                                                                //\n  });                                                         