sh(obj);                                                                                           // 363\n            count++;                                                                                                   // 364\n          }                                                                                                            //\n        }                                                                                                              //\n        Session.setPersistent('arrayGrpLen', array.length);                                                            // 367\n        return array;                                                                                                  // 368\n      }                                                                                                                //\n                                                                                                                       //\n      return getStatsGroup;                                                                                            //\n    }(),                                                                                                               //\n                                                                                                                       //\n    'getLengths': function () {                                                                                        // 371\n      function getLengths() {                                                                                          // 371\n        var obj = { a1: Session.get('arrayIndLen') > 0, a2: Session.get('arrayGrpLen') > 0 };                          // 372\n        console.log(obj + ' ...........obj');                                                                          // 373\n        return obj;                                                                                                    // 374\n      }         