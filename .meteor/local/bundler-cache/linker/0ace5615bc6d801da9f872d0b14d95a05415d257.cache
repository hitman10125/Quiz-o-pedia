                   // 228\n    time_left: function () {                                                                                           // 229\n      function time_left() {                                                                                           // 229\n        var time = Template.instance().time_left.get();                                                                // 230\n        Session.set('timeLeft', time);                                                                                 // 231\n        var minutes = (Math.floor(time / 60) < 10 ? '0' : '') + Math.floor(time / 60);                                 // 232\n        var seconds = (time % 60 < 10 ? '0' : '') + time % 60;                                                         // 233\n        if (time <= 0) {                                                                                               // 234\n          Meteor.clearInterval(Session.get('updateWaitingTimeLeft'));                                                  // 235\n          Meteor.clearInterval(Session.get('updateTimeLeft'));                                                         // 236\n          Session.clear('final_answer');                                                                               // 237\n          Session.clear('subscribed');                                                                                 // 238\n          $('.scoreCard').trigger('click');                                                                            // 239\n        }                                                                                                              //\n        Session.update('quiz_time_left', minutes + \":\" + seconds);                                                     // 241\n        Session.makePersistent('quiz_time_left');                                                                      // 242\n        return minutes + \":\" + seconds;                             