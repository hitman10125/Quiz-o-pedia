{                                                                  // 335\n          if (statistics[i].type === 'Individual') {                                                                   // 336\n            arr1[count] = statistics[i].category;                                                                      // 337\n            arr2[count] = statistics[i].score;                                                                         // 338\n            arr3[count] = statistics[i].takenOn;                                                                       // 339\n            var obj = { arr1: arr1[count], arr2: arr2[count], arr3: arr3[count] };                                     // 340\n            array.push(obj);                                                                                           // 341\n            count++;                                                                                                   // 342\n          }                                                                                                            //\n        }                                                                                                              //\n        Session.setPersistent('arrayIndLen', array.length);                                                            // 345\n        return array;                                                                                                  // 346\n      }                                                                                                                //\n                                                                                                                       //\n      return getStatsIndividual;                                                                                       //\n    }(),                                                                                                               //\n                                            