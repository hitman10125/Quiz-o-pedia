                       // 202\n                                                                                                                       //\n          if (groupQuizzes[i].waiting_time <= 0) {                                                                     // 204\n            console.log('Hello');                                                                                      // 205\n            groupQuizzes.splice(i, 1);                                                                                 // 206\n            i = -1;                                                                                                    // 207\n          }                                                                                                            //\n        }                                                                                                              //\n        return groupQuizzes.length > 0;                                                                                // 211\n      }                                                                                                                //\n                                                                                                                       //\n      return totalGroupQuizzes;                                                                                        //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.events({                                                                                             // 215\n    'click input': function () {                                                                    