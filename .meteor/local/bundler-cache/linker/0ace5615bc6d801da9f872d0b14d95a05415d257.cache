                                                                                                 //\n                                                                                                                       //\n        Meteor.call(\"getResponseSet\", function (error, result) {                                                       // 403\n          if (error) {                                                                                                 // 404\n                                                                                                                       //\n            console.log(error.reason);                                                                                 // 406\n          } else {                                                                                                     //\n            Session.setPersistent('selectedScore1', result);                                                           // 410\n            console.log(Session.get('selectedScore1'));                                                                // 411\n          }                                                                                                            //\n        });                                                                                                            //\n                                                                                                                       //\n        Meteor.call(\"getActualScore\", function (error, result) {                                                       // 415\n          if (error) {                                                                                                 // 416\n                                                                                                                       //\n            console.log(error.reason);                                                                                 // 418\n          } else {