                                    //\n  Template.waiting_time.onCreated(function () {                                                                        // 98\n    this.time_left = new ReactiveVar(300);                                                                             // 99\n    Session.clear('updateWaitingTimeLeft');                                                                            // 100\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionSelect.onRendered(function () {                                                                     // 103\n    Meteor.clearInterval(Session.get('updateTimeLeft'));                                                               // 104\n    var self = this;                                                                                                   // 105\n                                                                                                                       //\n    Meteor.call(\"getQuiz\", function (error, result) {                                                                  // 107\n      if (error) console.log(error.reason);else {                                                                      // 108\n        quizQuestions = result;                                                                                        // 111\n        Session.set('selectedQuestion', '1');                                                                          // 112\n        changeAnswer('1');                                                                                             // 113\n      }                                                                                                                //\n    });                                                                                                                //\n                                                                                                                       //\n    Meteor.call(\"getTimer\", function (error, result) {                                                                 // 119\n      if (error) console.log(error.reason);else {                                                                      // 120\n        self.time_left.set(result);                                                                                    // 123\n      }                                                                                                                //\n    });                                                                                                                //\n                                                                                                                       //\n    updateTimeLeft = Meteor.setInterval(function () {                                                                  // 127\n      self.time_left.set(self.time_left.get() - 1);                                                                    // 128\n      Meteor.call('setTimer', self.time_left.get());                                                                   // 129\n    }, 1000);                                                                                                          //\n    Session.setPersistent('updateTimeLeft', updateTimeLeft);                                                           // 131\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionSelect.onDestroyed(function () {                                                                    // 135\n                                                                     