go('quiz');                                                                                             // 307\n      }                                                                                                                //\n                                                                                                                       //\n      return clickHome;                                                                                                //\n    }(),                                                                                                               //\n                                                                                                                       //\n    'click #addquiz': function () {                                                                                    // 310\n      function clickAddquiz() {                                                                                        // 310\n                                                                                                                       //\n        Router.go('addquiz');                                                                                          // 312\n        //event.preventDefault();                                                                                      //\n        var que_array = new Array();                                                                                   // 310\n        var creater = event.target.creater.value;                                                                      // 315\n        var category = event.target.category.value;                                                                    // 316\n                                                                                                                       //\n        for (var i = 1; i <= 10; i++) {                                                                                // 318\n          eval(\"var que\" + \"= event.target.question\" + i + \".value\");                                                  // 319\n          eval(\"var ans\" + \"= event.target.question\" + i + \"_answer.value\");                                           // 320\n                                                                                                                       //\n          for (var op = 1; op <= 4; op++) {                                                                            // 322\n            eval(\"var opt\" + op + \"= event.target.question\" + i + \"_option\" + op + \".value\");                          // 323\n          }                                                                                                            //\n          que_array.push({ question: que, option1: opt1, option2: opt2, option3: opt3, option4: opt4, answer: ans });  // 325\n        }                                                                                                              //\n        //console.log(que_object);                                                                                     //\n        Meteor.call(\"addQuiz\", creater, category, que_array);                                                          // 310\n      }                                                                                                                //\n                                                                                                                       //\n      return clickAddquiz;                                                                                             //\n    }()                                                                                                                //\n                                                                                                                       //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.profile1.helpers({                                                                                          // 334\n    'getProfileData': function () {                                                                                    // 335\n      function getProfileData() {                                                                                      // 335\n        return Session.get('profileData');                                                                             // 336\n      }                                                                                                                //\n                                                                                                                       //\n      return getProfileData;                                                                                           //\n    }(),                                                                                                               //\n                                                                                                                       //\n    'getStatsIndividual': function () {                                                                                // 339\n      function getStatsIndividual() {                                                                                  // 339\n        var statistics = Session.get('profileData').stats;                                                             // 340\n        var count = 0;                                                                                                 // 341\n        var arr1 = [];                                                                                                 // 342\n        var arr2 = [];                                                                                                 // 343\n        var arr3 = [];                                                                                                 // 344\n        var array = [];                                                                                                // 345\n                                                                                                                       //\n        for (var i = 0; i < statistics.length; i++) {                                                                  // 347\n          if (statistics[i].type === 'Individual') {                                                                   // 348\n            arr1[count] = statistics[i].category;                                                                      // 349\n            arr2[count] = statistics[i].score;                                                                         // 350\n            arr3[count] = statistics[i].takenOn;                                                                       // 351\n            var obj = { arr1: arr1[count], arr2: arr2[count], arr3: arr3[count] };                                     // 352\n            array.push(obj);                                                                                           // 353\n            count++;                                                                                                   // 354\n          }                                                                                                            //\n        }                                                                                                              //\n        Session.setPersistent('arrayIndLen', array.length);                                                            // 357\n        return array;                                                                                                  // 358\n      }                                                                                                                //\n                                                                                                                       //\n      return getStatsIndividual;                                                                                       //\n    }(),                                                                                                               //\n                                                                                                                       //\n    'getStatsGroup': function () {                                                                                     // 361\n      function getStatsGroup() {                                                                                       // 361\n        var statistics = Session.get('profileData').stats;                                                             // 362\n        var count = 0;                                                                                                 // 363\n        var arr1 = [];                                                                                                 // 364\n        var arr2 = [];                                                                                                 // 365\n        var arr3 = [];                                                                                                 // 366\n        var array = [];                                                                                                // 367\n                                                                                                                       //\n        for (var i = 0; i < statistics.length; i++) {                                                                  // 369\n          if (statistics[i].type === 'Group') {                                                                        // 370\n            arr1[count] = statistics[i].category;                                                                      // 371\n            arr2[count] = statistics[i].score;                                                                         // 372\n            arr3[count] = statistics[i].takenOn;                                                                       // 373\n            var obj = { arr1: arr1[count], arr2: arr2[count], arr3: arr3[count] };                                     // 374\n            array.push(obj);                                                                                           // 375\n            count++;                                                                                                   // 376\n          }                                                                                                            //\n        }                                                                                                              //\n        Session.setPersistent('arrayGrpLen', array.length);                                                            // 379\n        return array;                                                                                                  // 380\n      }                                                                                                                //\n                                                                                                                       //\n      return getStatsGroup;                                                                                            //\n    }(),                                                                                                               //\n                                                                                                                       //\n    'getLengths': function () {                                                                                        // 383\n      function getLengths() {                                                                                          // 383\n        var obj = { a1: Session.get('arrayIndLen') > 0, a2: Session.get('arrayGrpLen') > 0 };                          // 384\n        console.log(obj + ' ...........obj');                                                                          // 385\n        return obj;                                                                                                    // 386\n      }                                                                                                                //\n                                                                                                                       //\n      return getLengths;                                                                                               //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  /* **************************************************************************************************** */           //\n                                                                                                                       //\n  Template.questionShow.events({                                                                                       // 7\n    'click input': function () {                                                                                       // 393\n      function clickInput(event, template) {                                                                           // 393\n        Meteor.call('setResponse', parseInt(Session.get('selectedQuestion')), event.target.value);                     // 394\n      }                                                                                   