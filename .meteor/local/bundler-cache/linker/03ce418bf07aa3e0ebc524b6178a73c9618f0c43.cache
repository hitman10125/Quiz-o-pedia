                                                        // 155\n      }                                                                                                                //\n    });                                                                                                                //\n                                                                                                                       //\n    updateWaitingTimeLeft = Meteor.setInterval(function () {                                                           // 159\n      self.time_left.set(self.time_left.get() - 1);                                                                    // 160\n      Meteor.call('setWaitingTimer', self.time_left.get());                                                            // 161\n    }, 1000);                                                                                                          //\n    Session.set('updateWaitingTimeLeft', updateWaitingTimeLeft);                                                       // 163\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionSelect.events({                                                                                     // 169\n    'click input': function () {                                                                                       // 170\n      function clickInput(event, template) {                                                                           // 170\n        Session.set('selectedQuestion', event.target.value);                                                           // 171\n        changeAnswer(event.target.value);                                                                              // 172\n      }                                                                                                                //\n                                                                                                                       //\n      return clickInput;                                                                                               //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.helpers({                                                                                            // 176\n    'showGroupQuizzes': function () {                                                                                  // 177\n      function showGroupQuizzes() {                                                                                    // 177\n        var groupQuizzes = ThisResponse.find({ type: 'Group' }).fetch();                                               // 178\n        var headrushQuiz = null;                                                                                       // 179\n        var time = new Date();                                                                                         // 180\n        for (i = 0; i < groupQuizzes.length; i++) {                                                                    // 181\n                                                                                                                       //\n          if (groupQuizzes[i].waiting_time <= 0) {                                                                     // 183\n            console.log('Hello');                                                                                      // 184\n            groupQuizzes.splice(i, 1);                                                                                 // 185\n            i = -1;                                                                                                    // 186\n          }                                                                                                            //\n          // if(groupQuizzes[i].conductedByHeadRush){                                                                  //\n          //   headrushQuiz = groupQuizzes[i];                                                                         //\n          //   groupQuizzes.splice(i, 1);                                                                              //\n          //   i=-1;                                                                                                   //\n          // }                                                                                                         //\n        }                                                                                                              // 181\n        return groupQuizzes;                                                                                           // 195\n      }                                                                                                                //\n                                                                                                                       //\n      return showGroupQuizzes;                                                                                         //\n    }(),                                                                                                               //\n    'totalGroupQuizzes': function () {                                                                                 // 197\n      function totalGroupQuizzes() {                                                                                   // 197\n        var groupQuizzes = ThisResponse.find({ type: 'Group' }).fetch();                                               // 198\n        var time = new Date();                                                                                         // 199\n        for (i = 0; i < groupQuizzes.length; i++) {                                                                    // 200\n                                                                                                                       //\n          if (groupQuizzes[i].waiting_time <= 0) {                                                                     // 202\n            console.log('Hello');                                                                                      // 203\n            groupQuizzes.splice(i, 1);                                                                                 // 204\n            i = -1;                                                                                                    // 205\n          }                                                                                                            //\n        }                                                                                                              //\n        return groupQuizzes.length > 0;                                                                                // 209\n      }                                                                                                                //\n                                                                                                                       //\n      return totalGroupQuizzes;                                                                                        //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.online.events({                                                                                             // 213\n    'click input': function () {                                                                                       // 214\n      function clickInput() {                                                                                          // 214\n        event.preventDefault();                                                                                        // 215\n                                                                                                                       //\n        var responseID = $(event.target).attr('id');                                                                   // 217\n        var subscribed = $(event.target).attr('value');                                                                // 218\n        console.log(subscribed);                                                                                       // 219\n        Session.setPersistent('subscribed', subscribed);                                                               // 220\n                                                                                                                       //\n        Meteor.call('setGroupResponseId', responseID);                                                                 // 222\n      }                                                                                                                //\n                                                                                                                       //\n      return clickInput;                                                                                               //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.questionSelect.helpers({                                                                                    // 226\n    time_left: function () {                                                                                           // 227\n      function time_left() {                                                                                           // 227\n        var time = Template.instance().time_left.get();                                                                // 228\n        Session.set('timeLeft', time);                                                                                 // 229\n        var minutes = (Math.floor(time / 60) < 10 ? '0' : '') + Math.floor(time / 60);                                 // 230\n        var seconds = (time % 60 < 10 ? '0' : '') + time % 60;                                                         // 231\n        if (time <= 0) {                                                                                               // 232\n          Meteor.clearInterval(Session.get('updateWaitingTimeLeft'));                                                  // 233\n          Meteor.clearInterval(Session.get('updateTimeLeft'));                                                         // 234\n          Session.clear('final_answer');                                                                               // 235\n          Session.clear('subscribed');                                                                                 // 236\n          $('.scoreCard').trigger('click');                                                                            // 237\n        }                                                                                                              //\n        Session.update('quiz_time_left', minutes + \":\" + seconds);                                                     // 239\n        Session.makePersistent('quiz_time_left');                                                                      // 240\n        return minutes + \":\" + seconds;                                                                                // 241\n      }                                                                                                                //\n                                                                                                                       //\n      return time_left;                                                                                                //\n    }(),                                                                                                               //\n    'ended': function () {                                                                                             // 243\n      function ended() {                                                                                               // 243\n        return Session.get('timeLeft') <= 0;                                                                           // 244\n      }                                                                                                                //\n                                                                                                                       //\n      return ended;                                                                                                    //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.waiting_time.helpers({                                                                                      // 248\n    time_left: function () {                                                                                           // 249\n      function time_left() {                                                                                           // 249\n        var time = Template.instance().time_left.get();                                                                // 250\n        Session.set('timeLeft', time);                                                                                 // 251\n        var minutes = (Math.floor(time / 60) < 10 ? '0' : '') + Math.floor(time / 60);                                 // 252\n        var seconds = (time % 60 < 10 ? '0' : '') + time % 60;                                                         // 253\n        if (time <= 0) {                                                                                               // 254\n          Meteor.clearInterval(Session.get('updateWaitingTimeLeft'));                                                  // 255\n          Meteor.clearInterval(Session.get('updateTimeLeft'));                                                         // 256\n          Session.clear('final_answer');                                                                               // 257\n          Session.clear('subscribed');                                                                                 // 258\n        }                                                                                                              //\n        return minutes + \":\" + seconds;                                                                                // 260\n      }                                                                                                                //\n                                                                                                                       //\n      return time_left;                                                                                                //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.navigation.helpers({                                                                                        // 264\n    'user': function () {                                                                                              // 265\n      function user() {                                                                                                // 265\n        console.log(Meteor.users.findOne(Meteor.userId()).username + \"........xxx......\");                             // 266\n        return Meteor.users.find({ _id: Meteor.userId() });                                                            // 267\n      }                                                                                                                //\n                                                                                                                       //\n      return user;                                                                                                     //\n    }(),                                                                                                               //\n                                                                                                                       //\n    'isAdmin': function () {                                                                                           // 270\n      function isAdmin() {                                                                                             // 270\n        return Meteor.users.findOne(Meteor.userId()).username === 'admin';                                             // 271\n      }                                                                                                                //\n                                                                                                                       //\n      return isAdmin;                                                                                                  //\n    }()                                                                                                                //\n  });                                                                                                                  //\n                                                                                                                       //\n  Template.navigation.events({                                                                                         // 275\n    'click #logoutbtn': function () {                                                                                  // 276\n      function clickLogoutbtn() {                                                                                      // 276\n        Meteor.call('endquiz', Meteor.userId());                                                                       // 277\n        Meteor.clearInterval(Session.get('updateWaitingTimeLeft'));                                                    // 278\n        console.log();                                                                                                 // 279\n        Meteor.clearInterval(Session.get('updateTimeLeft'));                                                           // 280\n        Session.clear('final_answer');                                                                                 // 281\n        Session.clear('subscribed');                                                                                   // 282\n        Meteor.logout();                                                                                               // 283\n      }                                                                                                                //\n                                                                                                                       //\n      return clickLogoutbtn;                                                                                           //\n    }(),                                                                                                               //\n    'click #profileButton': function () {                                                                              // 285\n      function clickProfileButton() {                                                                                  // 285\n        Meteor.call(\"getProfileData\", function (error, result) {                                                       // 286\n          if (error) {                                                                                                 // 287\n            //console.log(\"Chu\");                                                                                      //\n            console.log(error.reason);                                                                                 // 289\n          } else {                                                                                                     //\n            Session.set('profileData', result);                                                                        // 293\n            Router.go('profile');                                                                                      // 294\n            //console.log(Session.get('selectedScore1'));                                                              //\n          }                                                                                                            // 292\n   