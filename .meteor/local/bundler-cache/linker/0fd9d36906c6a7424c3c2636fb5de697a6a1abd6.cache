onseSet\", function (error, result) {                                                       // 303\n          if (error) {                                                                                                 // 304\n            //console.log(\"Chu\");                                                                                      //\n            console.log(error.reason);                                                                                 // 306\n          } else {                                                                                                     //\n            Session.set('selectedScore1', result);                                                                     // 310\n            console.log(Session.get('selectedScore1'));                                                                // 311\n          }                                                                                                            //\n        });                                                                                                            //\n                                                                                                                       //\n        Meteor.call(\"getActualScore\", function (error, result) {                                                       // 315\n          if (error) {                                                                                                 // 316\n                                                                                                                       //\n            console.log(error.reason);                                                                                 // 318\n          } else {                                                                                                     //\n                                                                                                                       //\n            Session.set